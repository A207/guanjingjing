package test;

public class Exe03 {

	public static void main(String[] args) {
		byte a1 = 1;
		byte a2 = 1;
		System.out.println(a1+"+"+a2+"="+(a1+a2));
		short b1 = 2;
		short b2 = 2;
		System.out.println(b1+"-"+b2+"="+(b1-b2));
		int c1 = 3;
		int c2 = 3;
		System.out.println(b1+"*"+b2+"="+(c1*c2));
		long d1 = 4;
		long d2 = 4;
		System.out.println(b1+"/"+b2+"="+(d1/d2));
		float e = 5f;
		double f = 6;
		System.out.println(e+"\n"+f);
	}


}
