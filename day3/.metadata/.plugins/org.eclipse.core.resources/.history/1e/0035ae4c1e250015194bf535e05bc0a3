	public Person2(String name, int age, String address){
		this(name, age);//调用当前类的带两个参数的构造方法
		this,address = address;
	}
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
